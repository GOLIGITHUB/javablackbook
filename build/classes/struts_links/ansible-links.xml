	<result name="success"  type="tiles">ansible-Loop</result>
	</action>
	
	<action name="ansible-Condition">
	<result name="success"  type="tiles">ansible-Condition</result>
	</action>
	
	<action name="ansible-Jinja2-Template">
	<result name="success"  type="tiles">ansible-Jinja2-Template</result>
	</action>
	
