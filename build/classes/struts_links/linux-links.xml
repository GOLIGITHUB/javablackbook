<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
    "http://struts.apache.org/dtds/struts-2.1.7.dtd">
	
	<struts>
	<constant name="struts.devMode" value="true"/>
	<package name="linux" extends="struts-default" namespace="/linux">
	
	<result-types>
		<result-type name="tiles" class="org.apache.struts2.views.tiles.TilesResult" />
	</result-types>

	<action name="linux-home-page">
	<result name="success"  type="tiles">linux-home-page</result>
	</action>
	
	<action name="linux-commands-basic-commands">
	<result name="success"  type="tiles">linux-commands-basic-commands</result>
	</action>
	
	<action name="linux-user-and-group-commands">
	<result name="success"  type="tiles">linux-user-and-group-commands</result>
	</action>
	
	<action name="linux-commands-process-commands">
	<result name="success"  type="tiles">linux-commands-process-commands</result>
	</action>
	
	<action name="testing">
	<result name="success"  type="tiles">testing</result>
	</action>
				
</package>
</struts>